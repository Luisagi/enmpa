Package: enmpa
Title: Ecological Niche Modeling using Presence-Absence Data
Version: 0.1.3
Authors@R: c(
    person("Luis F.", "Arias-Giraldo", email = "lfarias.giraldo@gmail.com",
    role = c("aut", "cre"), comment = c(ORCID = "0000-0003-4861-8064")),
